{
  "name": "catalogues",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "preview": "vite preview",
    "lint:css": "npx stylelint **/*.scss",
    "lint:ts": "npx eslint src --ext js,jsx,ts,tsx",
    "lint": "npm run lint:ts && npm run lint:css",
    "format:fix": "npx prettier src --write",
    "format": "npx prettier src --check",
    "check": "npm run lint && npm run format && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@adrianbielawski/orderable-list": "^0.1.17",
    "@adrianbielawski/use-swipe": "^0.1.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.71.0",
    "axios-observable": "^2.0.0",
    "classnames": "^2.3.2",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.8.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-animate-height": "^3.2.2",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-player": "^2.12.0",
    "react-redux": "^8.1.2",
    "react-resize-detector": "^9.1.0",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.1",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.11",
    "@types/leaflet": "^1.9.4",
    "@types/lodash": "^4.14.197",
    "@types/mime-types": "^2.1.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.5.6",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-leaflet": "^3.0.0",
    "@types/react-redux": "^7.1.26",
    "@types/react-resize-detector": "^6.1.0",
    "@types/react-transition-group": "^4.4.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-favicons-inject": "^2.2.0"
  }
}
